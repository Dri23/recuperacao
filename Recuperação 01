valor_hora = float(input("Valor da hora trabalhada: "))
horas_trabalhadas = int(input("Número de horas trabalhadas no mês: "))
salario_bruto = valor_hora*horas_trabalhadas
desconto_inss = (7.5 / 100.0) * salario_bruto
desconto_gps = (11.0 / 100.0) * salario_bruto
salario_liquido = salario_bruto - (desconto_inss + desconto_gps)

print("Seu salário Bruto: {0}" .format(salario_bruto))
print("Desconto de INSS: {0}" .format(desconto_inss))
print("Desconto GPS: {0}" .format(desconto_gps))
print("Salário Líquido: {0}" .format(salario_liquido))
